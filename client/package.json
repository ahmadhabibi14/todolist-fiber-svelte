{
  "name": "svelte-mpa-app",
  "version": "0.0.0",
  "scripts": {
    "start": "npm run dev",
    "tailwind:watch": "npx tailwindcss -i ./css/main.css -o ./css/main.tmp.css --watch",
    "watch": "npm run build -- --watch",
    "dev": "concurrently 'pnpm run tailwind:watch' 'node build.js --watch --serve'",
    "build": "node build.js",
    "build:prod": "node build.js --minify",
    "serve": "node build.js --serve"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.0",
    "esbuild": "^0.16.17",
    "esbuild-svelte": "^0.7.3",
    "five-server": "^0.1.9",
    "lodash": "^4.17.21",
    "parse5": "^7.1.2",
    "postcss": "^8.4.24",
    "prettier": "^2.8.7",
    "svelte": "^3.58.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.5"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 120,
    "arrowParens": "avoid"
  }
}
